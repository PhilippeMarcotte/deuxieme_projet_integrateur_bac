<Window x:Class="InterfaceGraphique_ClientLourd.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:InterfaceGraphique_ClientLourd"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Button x:Name="ConnectButton" Content="Connection" HorizontalAlignment="Left" Margin="34,230,0,0" VerticalAlignment="Top" Width="75" Click="button_Click"/>
        <PasswordBox x:Name="passwordBox" HorizontalAlignment="Left" Margin="34,158,0,0" VerticalAlignment="Top" Width="176"/>
        <Image x:Name="image" HorizontalAlignment="Left" Height="244" Margin="263,32,-20,0" VerticalAlignment="Top" Width="274" OpacityMask="#FFC12525" Source="koala.jpg"/>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="23" Margin="34,72,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="176"/>
        <Label x:Name="label" Content="Label" HorizontalAlignment="Left" Margin="34,100,0,0" VerticalAlignment="Top" Width="176"/>
        <Label x:Name="label1" Content="Label" HorizontalAlignment="Left" Margin="34,176,0,0" VerticalAlignment="Top" Width="176"/>
        <Label x:Name="label2" Content="Username" HorizontalAlignment="Left" Margin="34,46,0,0" VerticalAlignment="Top" Width="176"/>
        <Label x:Name="label3" Content="Password" HorizontalAlignment="Left" Margin="34,132,0,0" VerticalAlignment="Top" Width="176"/>
    </Grid>
</Window>
